<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<Pane fx:id="root" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="286.0" prefWidth="393.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.gui.Controller">
    <Button fx:id="btnStart" layoutX="345.0" layoutY="14.0" mnemonicParsing="false" onAction="#start" text="开始"/>
    <Button fx:id="btnStop" layoutX="345.0" layoutY="45.0" mnemonicParsing="false" onAction="#stop" text="暂停"/>
    <TabPane fx:id="tabPane" prefHeight="259.0" prefWidth="196.0" tabClosingPolicy="UNAVAILABLE">
        <Tab fx:id="tanSuoPane" onSelectionChanged="#tanSuoModel" text="探索">
            <Pane prefHeight="239.0" prefWidth="196.0">
                <RadioButton id="danRenTanSuo" fx:id="danRenTanSuo" layoutX="83.0" layoutY="10.0"
                             mnemonicParsing="false" selected="true" text="单人"/>
                <RadioButton fx:id="zuDuiTanSuo" layoutX="137.0" layoutY="10.0" mnemonicParsing="false"
                             text="组队"/>
                <Label layoutX="14.0" layoutY="10.0" text="探索方式："/>
                <Label layoutX="14.0" layoutY="30.0" text="结界突破："/>
                <RadioButton fx:id="daJieJie" layoutX="83.0" layoutY="30.0" mnemonicParsing="false"
                             text="打"/>
                <RadioButton fx:id="buDaJieJie" layoutX="137.0" layoutY="30.0" mnemonicParsing="false"
                             text="不打"/>
                <Label layoutX="14.0" layoutY="50.0" text="狗粮更换："/>
                <RadioButton fx:id="huanGouLiang" layoutX="83.0" layoutY="50.0" mnemonicParsing="false"
                             text="换"/>
                <RadioButton fx:id="buHuanGouLiang" layoutX="137.0" layoutY="50.0"
                             mnemonicParsing="false" text="不换"/>
                <Label layoutX="14.0" layoutY="74.0" text="探索次数："/>
                <TextField fx:id="tanSuoTimes" layoutX="77.0" layoutY="70.0" prefHeight="23.0"
                           prefWidth="58.0"/>
                <Label layoutX="144.0" layoutY="74.0" text="次"/>
                <Label layoutX="20.0" layoutY="98.0" text="1、0次表示不限制探索次数"/>
                <Label layoutX="20.0" layoutY="113.0" text="2、暂停后再开始将重新计数"/>
                <Label layoutX="14.0" layoutY="137.0" text="注意事项："/>
                <Label layoutX="20.0" layoutY="152.0" text="1、探索输出式神应处于右下角"/>
                <Label layoutX="20.0" layoutY="167.0" text="2、需要更换狗粮的请解锁阵容"/>
                <Label layoutX="20.0" layoutY="181.0" text="3、想要打结界请提前配好御魂"/>
                <Label layoutX="39.0" layoutY="194.0" text="并锁定阵容"/>
            </Pane>
        </Tab>
        <Tab fx:id="yuHunPane" onSelectionChanged="#yuHunModel" text="御魂">
            <Pane prefHeight="215.0" prefWidth="196.0">
                <Label layoutX="14.0" layoutY="10.0" text="御魂模式："/>
                <Label layoutX="14.0" layoutY="30.0" text="御魂类型："/>
                <RadioButton fx:id="danRenYuHun" layoutX="83.0" layoutY="10.0" mnemonicParsing="false"
                             text="单人"/>
                <RadioButton fx:id="zuDuiYuHun" layoutX="137.0" layoutY="10.0" mnemonicParsing="false"
                             text="组队"/>
                <RadioButton fx:id="hunTuYuHun" layoutX="83.0" layoutY="30.0" mnemonicParsing="false"
                             text="魂土"/>
                <RadioButton fx:id="hunShiYuHun" layoutX="137.0" layoutY="30.0" mnemonicParsing="false"
                             text="魂十"/>
                <RadioButton fx:id="yeYuanHuoYuHun" layoutX="83.0" layoutY="50.0" mnemonicParsing="false"
                             text="业原火"/>
                <Label layoutX="60.0" layoutY="103.0" text="还没做好" textFill="#4f4fff">
                    <font>
                        <Font size="19.0"/>
                    </font>
                </Label>
            </Pane>
        </Tab>
        <Tab fx:id="yuLinPane" onSelectionChanged="#yuLinModel" text="御灵">
            <Pane prefHeight="200.0" prefWidth="200.0">
                <Label layoutX="62.0" layoutY="109.0" text="还没做好" textFill="#0015ff">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
            </Pane>
        </Tab>
        <Tab fx:id="jieJiePane" onSelectionChanged="#jieJieModel" text="结界"/>
        <Tab fx:id="baiGuiPane" onSelectionChanged="#baiGuiModel" text="百鬼">
            <Pane prefHeight="200.0" prefWidth="200.0">
                <Label layoutX="62.0" layoutY="103.0" text="还没做好" textFill="#0037ff">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
            </Pane>
        </Tab>
    </TabPane>
    <Label fx:id="logLable" layoutX="270.0" layoutY="158.0" text="日志打印"/>
    <TextArea fx:id="logArea" layoutX="196.0" layoutY="173.0" prefHeight="111.0" prefWidth="196.0"/>
    <ImageView fx:id="gifImg" fitHeight="150.0" fitWidth="140.0" layoutX="197.0" pickOnBounds="true"
               preserveRatio="true"/>
    <Label layoutX="30.0" layoutY="273.0" text="所有模式均可点击Esc暂停"/>
    <Label layoutX="30.0" layoutY="259.0" text="所有模式均可点击空格开始"/>
</Pane>
